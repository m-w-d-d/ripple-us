 <ul class="list--inline site-footer__social-icons social-icons site-footer__icon-list">
            
            {%- if settings.social_instagram_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link" href="{{ settings.social_instagram_link | escape }}" aria-describedby="a11y-external-message">
                  {%- include 'icon-instagram' -%}
                  <span class="icon__fallback-text">Instagram</span>
                </a>
              </li>
            {%- endif -%}
            
            {%- if settings.social_facebook_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link" href="{{ settings.social_facebook_link | escape }}" aria-describedby="a11y-external-message">
                  {%- include 'icon-facebook' -%}
                  <span class="icon__fallback-text">Facebook</span>
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_twitter_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link" href="{{ settings.social_twitter_link | escape }}" aria-describedby="a11y-external-message">
                  {%- include 'icon-twitter' -%}
                  <span class="icon__fallback-text">Twitter</span>
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_pinterest_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link" href="{{ settings.social_pinterest_link | escape }}" aria-describedby="a11y-external-message">
                  {%- include 'icon-pinterest' -%}
                  <span class="icon__fallback-text">Pinterest</span>
                </a>
              </li>
            {%- endif -%}
   
           {%- if settings.social_tiktok_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link tik-tok-li" href="{{ settings.social_tiktok_link | escape }}" aria-describedby="a11y-external-message">
                  {%- include 'icon-tiktok' -%}
                  <span class="icon__fallback-text">TikTok</span>
                </a>
              </li>
            {%- endif -%}
   					
            
            {%- if settings.social_tumblr_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link" href="{{ settings.social_tumblr_link | escape }}" aria-describedby="a11y-external-message">
                  {%- include 'icon-tumblr' -%}
                  <span class="icon__fallback-text">Tumblr</span>
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_snapchat_link != blank -%}
              <li>
                <a target="_blank" class="social-icons__link" href="{{ settings.social_snapchat_link | escape }}" aria-describedby="a11y-external-message">
