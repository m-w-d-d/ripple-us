<div id="wholesale-form" class="wholesale_form" style="background-color:{{ section.settings.wholesale_form_bg }};">
  <div class="wholesale_form_wrapper">
    {% if section.settings.wholesale_form_title != blank %}
    <h1 class="size_38 align_center">{{ section.settings.wholesale_form_title }}</h1>
    {% endif %}
    
    
    <div class="wholesale_form_div">
      {% comment %}
      <div class="form_flex_title">
        <div class="wholesale_form_field">
          {% if section.settings.section_one_form_title != blank %}
          <h2>{{ section.settings.section_one_form_title }}</h2>
          {% endif %}
        </div>
        <div class="wholesale_form_field">
          {% if section.settings.section_two_form_title != blank %}
          <h2>{{ section.settings.section_two_form_title }}</h2>
          {% endif %}
        </div>
      </div>
      {% endcomment %}
      <iframe src="https://dashcreativeshopifyscripts.co/ripple/ripple-wholesale-form/form12.php" style="width:100%; height:490px;" frameborder="0"></iframe>
      </div>
      
    </div>

    
  </div>


{% schema %}
  {
    "name": "Wholesale Form",
    "settings": [
	{
        "type": "color",
        "id": "wholesale_form_bg",
        "label": "Background color",
		"default": "#fff"
        },
		{
        "type": "text",
        "id": "wholesale_form_title",
        "label": "Title"
        },
		{
        "type": "text",
        "id": "section_one_form_title",
        "label": "Title One"
        },
		{
        "type": "text",
        "id": "section_two_form_title",
        "label": "Title Two"
        }

],
	"presets": [
    {
      "name": "Wholesale Form"
    }
  ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
