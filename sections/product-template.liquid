{% if product.metafields.accentuate.date_and_time != blank %}
<script>
  
  $(document).ready(function() {

  //var timing = "Nov 29 2021, 14:00:00";
  var timing = "{{ product.metafields.accentuate.date_and_time }}";
   if(timing!="")
 {
 // console.log(timing);
// Set the date we're counting down to
  var countDownDate = new Date(timing).getTime();

// Update the count down every 1 second
var x = setInterval(function() {

  // Get today's date and time
  var test = new Date().toLocaleString("en-US", {timeZone: "Europe/London"});
   var now = new Date(test).getTime();
    
  // Find the distance between now and the count down date
  var distance = countDownDate - now;
  
 // console.log(distance);
    
  // Time calculations for days, hours, minutes and seconds
  var days = Math.floor(distance / (1000 * 60 * 60 * 24));
  //console.log(days);
  var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
  
  
  
    var hr=0;
  if(days===1)
  {
  hr=24;
  }
   else
  {
  hr=days * 24;
  }
  
   
  
  var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
  var seconds = Math.floor((distance % (1000 * 60)) / 1000);
  
    var new_hours = Math.floor(hr)+Math.floor(hours); 
 // var new_hours = Math.floor(hours); 
  
   new_hour = "<span class='hour'>"+ new_hours + " H</span>";
   new_min = "<span class='minutes'>"+ minutes + " M</span>";
   new_sec = "<span class='seconds'>"+ seconds + " S</span>";
  
  // Output the result in an element with id="demo"
document.getElementById("demo_clock").innerHTML =  new_hour  + new_min  + new_sec;
  // If the count down is over, write some text 
 if(isNaN(distance))
 {
    clearInterval(x);
    document.getElementById("demo_clock").innerHTML = "";
       document.getElementById("clock_lable").innerHTML = "";

  } 
  
  else if(distance < 0)
  {
  
    clearInterval(x);
    document.getElementById("demo_clock").innerHTML = "";
        document.getElementById("clock_lable").innerHTML = "";


  }
  
}, 1000);
  }
  });
</script>

