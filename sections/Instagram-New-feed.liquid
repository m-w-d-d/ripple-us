<div class="instafeed_section">
<div class="insta_feed_section">
  

  <div id="insta-feeds">
{% for block in section.blocks %}
 <div class="insta_box_new">
   {% if block.settings.link != blank %}
   <a href="{{ block.settings.link }}" target="_blank">
     {% endif %}
     {% if block.settings.image != blank %}
        {%- assign img_url = block.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
        {% include 'image-style' with image: block.settings.image, width: 545, height: 545, small_style: true, wrapper_id: wrapper_id, img_id: img_id %}
        <div id="" class="feature-row__image-wrapper js">
          <div style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;">
            <img id=""
                 class="feature-row__image lazyload"
                 src="{{ block.settings.image | img_url: '200x200' }}"
                 data-src="{{ img_url }}"
                 data-widths="[180, 360, 540]"
                 data-aspectratio="{{ block.settings.image.aspect_ratio }}"
                 data-sizes="auto"           data-expand="-20"

                 alt="{{ block.settings.image.alt | escape }}">
          </div>
        </div>
     {% endif %}
   {% if block.settings.link != blank %}
     <div class="instafeed-overlay"></div>
   </a>
   {% endif %}
  </div>
{% endfor %}
    
    
    
    
