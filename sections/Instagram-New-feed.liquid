<script>
$(document).ready(function(){
  $('#insta-feeds').slick({
  dots: false,
  infinite: true,
  speed: 5000,
    centerMode: false,
  slidesToShow:4,
  slidesToScroll: 1,
    cssEase: 'linear',
    autoplay: true,
    autoplaySpeed: 0,
    initialSlide: 1,
  responsive: [
    {
      breakpoint: 1199,
      settings: {
        slidesToShow: 4,
        slidesToScroll: 1,
        infinite: true

      }
    },
    {
      breakpoint: 991,
      settings: {
        slidesToShow: 5,
        slidesToScroll: 1
      }
    },
    {
      breakpoint: 767,
      settings: {
        slidesToShow: 3,
        slidesToScroll: 1
      }
    }
    // You can unslick at a given breakpoint now by adding:
    // settings: "unslick"
    // instead of a settings object
  ]
});
  
  
  
    });
  </script>


{% schema %}
{
  "name": "Instagram Feed",
  "class": "index-section instagram-section",
  "settings": [
	{
      "type": "select",
      "id": "font_chnage",
      "label": "Text Font",
      "default": "heavy-font",
	  	
      "options": [
        {
          "label": "Bold",
          "value": "bold-font"
        },
        {
          "label": "Heavy Weight",
          "value": "heavy-font"
        }
      ]
    },
	{
       "type": "header",
       "content": "Title Color"
    },
    {
          "type": "color",
          "id": "title_first",
          "label": "Title gradient color first",
		  "default":"#445e83"
     },
	 {
          "type": "color",
          "id": "title_second",
          "label": "Title gradient color second",
		  "default":"#f8aeae"
     },
	{
      "type": "text",
      "id": "title",
      "label": {
        "de": "Titel",
        "en": "Heading",
        "es": "Título",
        "fr": "En-tête",
        "it": "Heading",
        "ja": "見出し",
        "nl": "Kop",
        "pt-BR": "Título",
        "zh-CN": "标题",
        "zh-TW": "標題"
      }
    }
  ],

  "blocks": [
    {
      "type": "insta_image",
      "name": "Image",
	   	
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image",
		  "info": "Size: 600 x 600px"
        },
        {
          "type": "text",
          "id": "link",
          "label": "Link"
        }
      ]
    }
  ],	
  "presets": [
    {
      "name":"Instagram New Feed",
      "category": "Images"
    }
  ]
}
{% endschema %}


